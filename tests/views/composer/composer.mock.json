	"horizontalTableMock": {
		"original": [{
			"type": "table-horizontal",
			"headers": ["One", "Two"],
			"data": [["Hello", "World"], ["Lorem", "Ipsum"]]
		}],
		"expected": "<table class=\"table-striped\"><tbody><tr><th><strong>One</strong></th><td>Hello</td><td>World</td></tr><tr><th><strong>Two</strong></th><td>Lorem</td><td>Ipsum</td></tr></tbody></table>"
	},
	"invalidMock": {
		"original": [{"type": "invalid"}],
		"expected": ""
	},
	"templates": {
		"group": {
			"original": [{"type": "group", "title": "Basic Information", "elems": []}],
			"expected": "<div class=\"form-group\"><label><h3>Basic Information</h3></label></div>"
		},
		"input": {
			"single": {
				"original": [{"type": "input", "title": "Name"}],
				"expected": "<label>Name</label><input type=\"text\" class=\"form-control\" placeholder=\"Name\">"
			}
		},
		"fullGroup": {
			"original": [{"type": "group", "title": "Basic Information", "extend": "none", "elems": [
							{"type": "input", "title": "Name"},
							{"type": "input", "title": "Group"}]
						}],
			"expected": "<div class=\"form-group\"><label><h3>Basic Information</h3></label><table><tbody><tr><td><label>Name</label></td><td><label>Group</label></td></tr><tr><td><input type=\"text\" class=\"form-control\" placeholder=\"Name\"></td><td><input type=\"text\" class=\"form-control\" placeholder=\"Group\"></td></tr></tbody></table></div>"
		},
		"extendvert": {
			"original": [{"type": "group", "title": "Questions", "extend": "vertical", "elems": [
							{"type": "input", "title": "Question"},
							{"type": "input", "title": "No. Choices"}]
						}],
			"expected": "<div class=\"form-group\"><label><h3>Questions</h3></label><table><tbody><tr><td><label>Question</label></td><td><label>No. Choices</label></td></tr><tr><td><input type=\"text\" class=\"form-control\" placeholder=\"Question\"></td><td><input type=\"text\" class=\"form-control\" placeholder=\"No. Choices\"></td></tr><tr><td><button class=\"btn btn-form btn-primary\" id=\"extend-questions\">Add</button></td></tr></tbody></table></div>"
		}
